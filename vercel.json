{
    "headers": [
        {
            "source": "/headers/(.*)",
            "headers": [
                {
                    "key": "x-pages-placeholders-msg",
                    "value": "$1"
                }
            ]
        },
        {
            "source": "/headers/.*.jpeg",
            "headers": [
                {
                    "key": "x-pages-placeholders-msg-2",
                    "value": "image jpeg"
                }
            ]
        },
        {
            "source": "/headers-placeholders/:id",
            "headers": [
                {
                    "key": "x-pages-placeholders-msg",
                    "value": ":id"
                }
            ]
        },
        {
            "source": "/headers-error-1",
            "headers": [
                {
                    "key": "中文",
                    "value": "error"
                }
            ]
        }
    ]
}
