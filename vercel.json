{
    "headers": [
        {
            "source": "/headers",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers"
                }
            ]
        },
        {
            "source": "/headers-1/(.*).jpeg",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-1, $1"
                }
            ]
        },
        {
            "source": "/headers-a/(.*)/name/(.*)",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-a, $1 $2"
                }
            ]
        },
        {
            "source": "/headers-b/(.*)/id/:id",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-b, $1 :id"
                }
            ]
        },
        {
            "source": "/headers-2/(.*)",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-2$1"
                }
            ]
        },
        {
            "source": "/headers-3/:id",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-3 :id"
                }
            ]
        },
        {
            "source": "/headers-4/:id/:name",
            "headers": [
                {
                    "key": "x-pages-msg",
                    "value": "/headers-4 :id :name"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/redirect/(.*)",
            "destination": "/redirect-1/$1",
            "statusCode": 302
        },
        {
            "source": "/redirect-a/(.*).jpeg",
            "destination": "/redirect-a2/$1.png",
            "statusCode": 301
        }    
    ]
}
